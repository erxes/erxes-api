{
  "extends": ["tslint:latest", "tslint-config-prettier"],
  "rules": {
    "object-literal-sort-keys": false,
    "no-console": false,
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore"
    ],
    "no-implicit-dependencies": [true, "dev"],
    "max-classes-per-file": [true, 5]
  }
}
